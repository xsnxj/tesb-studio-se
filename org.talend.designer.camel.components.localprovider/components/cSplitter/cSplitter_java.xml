<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"

		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20100401A"
		STARTABLE="false">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Routing</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR NAME="SPLIT" CTYPE="ROUTE" MIN_INPUT="1"
			MAX_INPUT="1" MAX_OUTPUT="1" MIN_OUTPUT="0" COLOR="7171FF" />
		<CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_ENDBLOCK" MIN_INPUT="0" MAX_INPUT="1"
			MAX_OUTPUT="1" MIN_OUTPUT="0" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="LANGUAGES" FIELD="CLOSED_LIST" REQUIRED="false"
			NUM_ROW="3">
			<ITEMS DEFAULT="XPATH">
				<ITEM NAME="NONE" VALUE="none" />
				<ITEM NAME="CONSTANT" VALUE="constant" />
				<ITEM NAME="HEADER" VALUE="header" />
				<ITEM NAME="PROPERTY" VALUE="property" />
				<ITEM NAME="SIMPLE" VALUE="simple" />
				<ITEM NAME="XPATH" VALUE="xpath" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="EXPRESSION" FIELD="TEXT" NUM_ROW="6"
			REQUIRED="true" GROUP="CORRELATION">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>


		<PARAMETER NAME="USE_NAMESPACES" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="7" GROUP="CORRELATION" SHOW_IF="LANGUAGES=='XPATH'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="NAMESPACES" FIELD="TABLE" NUM_ROW="8"
			NB_LINES="5" REQUIRED="true" GROUP="CORRELATION"
			SHOW_IF="(LANGUAGES=='XPATH') AND (USE_NAMESPACES == 'true')">
			<ITEMS>
				<ITEM NAME="PREFIX" VALUE="" />
				<ITEM NAME="URI" VALUE="" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="AGGREGATION_STRATEGY" FIELD="TEXT" NUM_ROW="9"
			REQUIRED="false">
			<DEFAULT>beans.MyAggregationStrategy</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PARALLET_PROCESSING" FIELD="CHECK" NUM_ROW="10" GROUP ="ARGUMENTS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="STOP_ON_EXCEPTION" FIELD="CHECK" NUM_ROW="11" GROUP ="ARGUMENTS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="STREAMING" FIELD="CHECK" NUM_ROW="12" GROUP ="ARGUMENTS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SHARE_UNIT_OF_WORK" FIELD="CHECK" NUM_ROW="13" GROUP ="ARGUMENTS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TIMEOUT" FIELD="TEXT" NUM_ROW="14" GROUP ="ARGUMENTS">
			<DEFAULT></DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
	</CODEGENERATION>

	<RETURNS />
</COMPONENT>
