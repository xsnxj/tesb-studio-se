	<CONNECTORS>
		<CONNECTOR NAME="IDEM" CTYPE="ROUTE" MIN_INPUT="1" MAX_INPUT="1"
			MAX_OUTPUT="1" MIN_OUTPUT="0" COLOR="086438"/>
		<CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1" MAX_OUTPUT="1" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="REPOSITORY_TYPE" FIELD="CLOSED_LIST"
			NUM_ROW="5" GROUP="REPOSITORY">
			<ITEMS DEFAULT="MEMORY">
				<ITEM NAME="MEMORY" VALUE="MEMORY" />
				<ITEM NAME="FILE" VALUE="FILE" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="FILE_STORE" FIELD="FILE" NUM_ROW="6"
			REQUIRED="true" GROUP="REPOSITORY" SHOW_IF="REPOSITORY_TYPE == 'FILE'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CACHE_SIZE" FIELD="TEXT" NUM_ROW="7"
			REQUIRED="true" GROUP="REPOSITORY"
			SHOW_IF="(REPOSITORY_TYPE == 'MEMORY') OR (REPOSITORY_TYPE == 'FILE')">
			<DEFAULT>200</DEFAULT>
		</PARAMETER>


		<PARAMETER NAME="USE_LANGUAGE" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="15">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="LANGUAGES" FIELD="CLOSED_LIST" REQUIRED="false"
			NUM_ROW="15" SHOW_IF="USE_LANGUAGE == 'true'">
			<ITEMS DEFAULT="XPATH">
				<ITEM NAME="NONE" VALUE="none" />
				<ITEM NAME="CONSTANT" VALUE="constant" />
				<!--<ITEM NAME="EL" VALUE="el" />
				<ITEM NAME="GROOVY" VALUE="groovy" />
				--><ITEM NAME="HEADER" VALUE="header" /><!--
				<ITEM NAME="JAVASCRIPT" VALUE="javaScript" />
				<ITEM NAME="JOSQL" VALUE="sql" />
				<ITEM NAME="JXPATH" VALUE="jxpath" />
				<ITEM NAME="MVEL" VALUE="mvel" />
				<ITEM NAME="OGNL" VALUE="ognl" />
				<ITEM NAME="PHP" VALUE="php" />
				<ITEM NAME="PYTHON" VALUE="python" />
				<ITEM NAME="RUBY" VALUE="ruby" />
				-->
				<ITEM NAME="PROPERTY" VALUE="property" />
				<ITEM NAME="SIMPLE" VALUE="simple" /><!--
				<ITEM NAME="SPEL" VALUE="spel" />
				<ITEM NAME="SQL" VALUE="sql" />
				--><ITEM NAME="XPATH" VALUE="xpath" />
				<!--<ITEM NAME="XQUERY" VALUE="xquery" />
			--></ITEMS>
		</PARAMETER>
		<PARAMETER NAME="EXPRESSION" FIELD="TEXT" NUM_ROW="15"
			REQUIRED="true" SHOW_IF="USE_LANGUAGE == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PREDICATE" FIELD="TEXT" NUM_ROW="15"
			REQUIRED="true" SHOW_IF="USE_LANGUAGE == 'true'">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="EAGER" FIELD="CHECK" NUM_ROW="16"
			REQUIRED="true" >
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SKIP_DUPLICATE" FIELD="CHECK" NUM_ROW="17"
			REQUIRED="true" >
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
