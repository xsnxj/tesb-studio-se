<COMPONENT>
	<HEADER PLATEFORM="ALL"
			SERIAL=""
			VERSION="0.102"
			STATUS="ALPHA"
			COMPATIBILITY="ALL"
			AUTHOR="ITOS"
			RELEASE_DATE="20050320A"
			SCHEMA_AUTO_PROPAGATE="true"
			STARTABLE="true"
			DATA_AUTO_PROPAGATE="false"
			HAS_CONDITIONAL_OUTPUTS="true" >
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>ESB/REST</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MIN_INPUT="0" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR NAME="RESPONSE" CTYPE="FLOW" COLOR="009900" MAX_OUTPUT="1" />
		<CONNECTOR NAME="FAULT" CTYPE="FLOW" LINE_STYLE="2" COLOR="cc0000" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="0" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="URL" FIELD="TEXT" REQUIRED_IF="SERVICE_LOCATOR == 'false'" NUM_ROW="10"
				SHOW_IF="SERVICE_LOCATOR == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PATH" FIELD="TEXT" REQUIRED="false" NUM_ROW="15">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="METHOD" FIELD="CLOSED_LIST" NUM_ROW="20">
			<ITEMS DEFAULT="GET">
				<ITEM NAME="GET" VALUE="GET" />
				<ITEM NAME="POST" VALUE="POST" />
				<ITEM NAME="PUT" VALUE="PUT" />
				<ITEM NAME="PATCH" VALUE="PATCH" />
				<ITEM NAME="DELETE" VALUE="DELETE" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="CONTENT_TYPE" FIELD="CLOSED_LIST" NUM_ROW="20"
				SHOW_IF="(METHOD == 'POST') OR (METHOD == 'PUT') OR (METHOD == 'PATCH')">
			<ITEMS DEFAULT="XML">
				<ITEM NAME="XML" VALUE="application/xml" />
				<ITEM NAME="JSON" VALUE="application/json" />
				<ITEM NAME="FORM" VALUE="application/x-www-form-urlencoded" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="ACCEPT_TYPE" FIELD="CLOSED_LIST" NUM_ROW="20"
				NOT_SHOW_IF="METHOD == 'DELETE'">
			<ITEMS DEFAULT="XML">
				<ITEM NAME="XML" VALUE="application/xml" />
				<ITEM NAME="JSON" VALUE="application/json" />
				<ITEM NAME="ANY" VALUE="*/*" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="QUERY" FIELD="TABLE" NB_LINES="4" NUM_ROW="30">
			<ITEMS>
				<ITEM NAME="NAME" VALUE='""' />
				<ITEM NAME="VALUE" VALUE='""' />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="INPUT_SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="31">
			<TABLE READONLY="true">
				<COLUMN NAME="body" TYPE="id_Document" READONLY="true" />
				<COLUMN NAME="string" TYPE="id_String" LENGTH="4048" READONLY="true" />
			</TABLE>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA_RESPONSE" FIELD="SCHEMA_TYPE" NUM_ROW="32" CONTEXT="RESPONSE">
			<TABLE READONLY="true">
				<COLUMN NAME="statusCode" TYPE="id_Integer" READONLY="true" />
				<COLUMN NAME="body" TYPE="id_Document" READONLY="true" />
				<COLUMN NAME="string" TYPE="id_String" LENGTH="4048" READONLY="true" />
			</TABLE>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA_FAULT" FIELD="SCHEMA_TYPE" NUM_ROW="33" CONTEXT="FAULT">
			<TABLE READONLY="true">
				<COLUMN NAME="errorCode" TYPE="id_Integer" READONLY="true" />
				<COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="1024" READONLY="true" />
			</TABLE>
		</PARAMETER>

		<PARAMETER NAME="SERVICE_LOCATOR" FIELD="CHECK" NUM_ROW="35" SHOW="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SERVICE_NAMESPACE" FIELD="TEXT" REQUIRED="true" NUM_ROW="35"
				SHOW_IF="SERVICE_LOCATOR == 'true'">
			<DEFAULT>"__PREF:org.talend.designer.esb.components.rs.provider:defaultSLNamespace__"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SERVICE_NAME" FIELD="TEXT" REQUIRED="true" NUM_ROW="35"
				SHOW_IF="SERVICE_LOCATOR == 'true'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SERVICE_ACTIVITY_MONITOR" FIELD="CHECK" NUM_ROW="38" SHOW="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="NEED_AUTH" FIELD="CHECK" REQUIRED="true" NUM_ROW="40" DYNAMIC_SETTINGS="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="AUTH_TYPE" FIELD="CLOSED_LIST" NUM_ROW="40" DYNAMIC_SETTINGS="true"
				SHOW_IF="((NEED_AUTH == 'true') OR ((NEED_AUTH != 'true') AND (NEED_AUTH != 'false')))">
			<ITEMS DEFAULT="SECURITY_BASIC">
				<ITEM NAME="SECURITY_BASIC" VALUE="BASIC" />
				<ITEM NAME="SECURITY_HTTP_DIGEST" VALUE="HTTP Digest" />
				<ITEM NAME="SECURITY_SAML" VALUE="SAML" />
				<ITEM NAME="SECURITY_OAUTH2_BEARER" VALUE="OAUTH2_BEARER" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="AUTH_USERNAME" FIELD="TEXT" REQUIRED="true" NUM_ROW="40"
				SHOW_IF="((NEED_AUTH == 'true') OR ((NEED_AUTH != 'true') AND (NEED_AUTH != 'false')))
						AND (AUTH_TYPE != 'OAUTH2_BEARER')">
			<DEFAULT>"username"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="AUTH_PASSWORD" FIELD="PASSWORD" REQUIRED="true" NUM_ROW="40"
				SHOW_IF="((NEED_AUTH == 'true') OR ((NEED_AUTH != 'true') AND (NEED_AUTH != 'false')))
						AND (AUTH_TYPE != 'OAUTH2_BEARER')">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="AUTH_BEARER_TOKEN" FIELD="TEXT" REQUIRED="true" NUM_ROW="40"
				SHOW_IF="((NEED_AUTH == 'true') OR ((NEED_AUTH != 'true') AND (NEED_AUTH != 'false')))
						AND (AUTH_TYPE != 'BASIC') AND (AUTH_TYPE != 'HTTP Digest') AND (AUTH_TYPE != 'SAML')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="NEED_AUTHORIZATION" FIELD="CHECK" REQUIRED="true" NUM_ROW="50"
				SHOW_IF="(IS_STUDIO_EE_VERSION == 'true')
						AND ((NEED_AUTH == 'true') OR ((NEED_AUTH != 'true') AND (NEED_AUTH != 'false')))
						AND (AUTH_TYPE != 'BASIC') AND (AUTH_TYPE != 'HTTP Digest') AND (AUTH_TYPE != 'OAUTH2_BEARER')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ROLE" FIELD="TEXT" NUM_ROW="55"
				SHOW_IF="(IS_STUDIO_EE_VERSION == 'true')
						AND ((NEED_AUTH == 'true') OR ((NEED_AUTH != 'true') AND (NEED_AUTH != 'false')))
						AND (AUTH_TYPE != 'BASIC') AND (AUTH_TYPE != 'HTTP Digest') AND (AUTH_TYPE != 'OAUTH2_BEARER')
						AND (NEED_AUTHORIZATION == 'true')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_BUSINESS_CORRELATION" FIELD="CHECK" NUM_ROW="71" SHOW="true" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CORRELATION_VALUE" FIELD="TEXT" NUM_ROW="71"
				SHOW_IF="USE_BUSINESS_CORRELATION == 'true'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="CHECK_SERVER_IDENTITY" FIELD="CHECK" NUM_ROW="75">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>

		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="99">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="LOG_MESSAGES" FIELD="CHECK" REQUIRED="true" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FOLLOW_REDIRECTS" FIELD="CHECK" REQUIRED="true" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="REDIRECT_NON_SAME_HOST" FIELD="CHECK" REQUIRED="true" NUM_ROW="20"
				SHOW_IF="FOLLOW_REDIRECTS == 'true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONVERT_RESPONSE_TO_DOM" FIELD="CHECK" REQUIRED="true" NUM_ROW="30"
				NOT_SHOW_IF="ACCEPT_TYPE == 'ANY'">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="UNWRAP_JSON_REQUEST" FIELD="CHECK" REQUIRED="true" NUM_ROW="40"
				SHOW_IF="(CONTENT_TYPE == 'JSON') AND ((METHOD == 'POST') OR (METHOD == 'PUT') OR (METHOD == 'PATCH'))">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="WRAP_JSON_RESPONSE" FIELD="CHECK" REQUIRED="true" NUM_ROW="40"
				SHOW_IF="ACCEPT_TYPE == 'JSON'">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONVERT_TYPES_TO_STRINGS" FIELD="CHECK" REQUIRED="true" NUM_ROW="41"
				SHOW_IF="((ACCEPT_TYPE == 'JSON') OR (ACCEPT_TYPE == 'ANY') OR (CONTENT_TYPE == 'JSON'))">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="HEADERS" FIELD="TABLE" NB_LINES="4" NUM_ROW="50">
			<ITEMS>
				<ITEM NAME="NAME" VALUE='""'/>
				<ITEM NAME="VALUE" VALUE='""'/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SERVICE_LOCATOR_CUSTOM_PROPERTIES" FIELD="TABLE" NUM_ROW="60"
				REQUIRED="false" NB_LINES="5" SHOW_IF="SERVICE_LOCATOR == 'true'">
			<ITEMS>
				<ITEM NAME="PROP_NAME" FIELD="TEXT" VALUE='"new custom property name"'/>
				<ITEM NAME="PROP_VALUE" FIELD="TEXT" VALUE='"new custom property value"'/>
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="SERVICE_ACTIVITY_MONITOR_CUSTOM_PROPERTIES" FIELD="TABLE" NUM_ROW="70"
				REQUIRED="false" NB_LINES="5" SHOW_IF="SERVICE_ACTIVITY_MONITOR == 'true'">
			<ITEMS>
				<ITEM NAME="PROP_NAME" FIELD="TEXT" VALUE='"new custom property name"'/>
				<ITEM NAME="PROP_VALUE" FIELD="TEXT" VALUE='"new custom property value"'/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="DISABLE_CHUNKED_ENCODING" FIELD="CHECK" REQUIRED="true" NUM_ROW="75"
				SHOW_IF="(METHOD == 'POST') OR (METHOD == 'PUT')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION_TIMEOUT" FIELD="TEXT" REQUIRED="false" NUM_ROW="80">
			<DEFAULT>30</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONNECTION_TIMEOUT_DESC" FIELD="LABEL" REQUIRED="true" NUM_ROW="80">
			<DEFAULT>(Studio Only, See Doc for Runtime)</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="RECEIVE_TIMEOUT" FIELD="TEXT" REQUIRED="false" NUM_ROW="80">
			<DEFAULT>60</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="RECEIVE_TIMEOUT_DESC" FIELD="LABEL" REQUIRED="true" NUM_ROW="80">
			<DEFAULT>(Studio Only, See Doc for Runtime)</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_PROXY" FIELD="CHECK" REQUIRED="true" NUM_ROW="90" DYNAMIC_SETTINGS="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_HOST" FIELD="TEXT" NUM_ROW="90"
				NOT_SHOW_IF="USE_PROXY == 'false'">
			<DEFAULT>"61.163.92.4"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PORT" FIELD="TEXT" NUM_ROW="90"
				NOT_SHOW_IF="USE_PROXY == 'false'">
			<DEFAULT>8080</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_USERNAME" FIELD="TEXT" NUM_ROW="90"
				NOT_SHOW_IF="USE_PROXY == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PASSWORD" FIELD="PASSWORD" NUM_ROW="90"
				NOT_SHOW_IF="USE_PROXY == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT MODULE="dom4j" MVN="${mvn-dom4j}" REQUIRED="true" />

			<IMPORT MODULE_GROUP="cxf-core" REQUIRED="true" />
			<IMPORT MODULE_GROUP="cxf-rt-rs-client" REQUIRED="true" />

			<IMPORT MODULE_GROUP="security-common" REQUIRED="true" />

			<IMPORT MODULE_GROUP="spring-context"
					REQUIRED_IF="(SERVICE_LOCATOR == 'true') OR (SERVICE_ACTIVITY_MONITOR == 'true')" />

			<IMPORT MODULE_GROUP="locator"
					REQUIRED_IF="SERVICE_LOCATOR == 'true'" />

			<IMPORT MODULE_GROUP="sam-agent"
					REQUIRED_IF="SERVICE_ACTIVITY_MONITOR == 'true'" />

			<IMPORT MODULE_GROUP="correlationid-policy"
					REQUIRED_IF="USE_BUSINESS_CORRELATION == 'true'" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="HEADERS" TYPE="java.util.Map&lt;String,java.util.List&lt;String&gt;&gt;" AVAILABILITY="FLOW" />
		<RETURN NAME="CORRELATION_ID" TYPE="id_String" AVAILABILITY="FLOW" />
	</RETURNS>
</COMPONENT>
