<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<COMPONENT>

	<HEADER AUTHOR="Talend" COMPATIBILITY="ALL" PLATEFORM="ALL"
		RELEASE_DATE="20100401A" SERIAL="" STARTABLE="true" STATUS="ALPHA"
		VERSION="0.102">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Transformation</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1"
			MIN_INPUT="0" MIN_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1" MAX_OUTPUT="0" />
	</CONNECTORS>

	<PARAMETERS>	
		<!-- USE EXIST ,provider -->
		<PARAMETER NAME="EXIST_ENDPOINT" FIELD="ROUTE_COMPONENT_TYPE" FILTER="cFlatPack"
			NUM_ROW="6" SHOW_IF="#LINK@CONNECTOR.IN.ANY &gt; '0'">
		</PARAMETER>

		<PARAMETER NAME="WARNING" FIELD="LABEL" NUM_ROW="5" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
			<DEFAULT>*Note: See the website http://flatpack.sourceforge.net/documentation/index.html for the full documentation on PZMAP file configuration.</DEFAULT>
		</PARAMETER>		
		<PARAMETER NAME="PZMAP_FILETYPE" FIELD="CLOSED_LIST" NUM_ROW="8" GROUP="PZMAP" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'" REQUIRED_IF="#LINK@CONNECTOR.IN.ANY == '0'">
			<ITEMS DEFAULT="file">
				<ITEM NAME="FILE" VALUE="file" />
				<ITEM NAME="REPO" VALUE="repo" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="PZMAP_FILENAME" FIELD="FILE" NUM_ROW="9" REQUIRED="false" GROUP="PZMAP" SHOW_IF="(PZMAP_FILETYPE == 'FILE') AND (#LINK@CONNECTOR.IN.ANY == '0')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PZMAP_FILE_REPO" FIELD="ROUTE_RESOURCE_TYPE" REPOSITORY_VALUE="ROUTER_RESOURCES"   NUM_ROW="9"	REQUIRED="false" GROUP="PZMAP" SHOW_IF="(PZMAP_FILETYPE == 'REPO') AND (#LINK@CONNECTOR.IN.ANY == '0')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ISFIXED" FIELD="RADIO" NUM_ROW="15" GROUP ="FILEFORMAT" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'" REQUIRED_IF="false">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ISDELIMITED" FIELD="RADIO" NUM_ROW="20" GROUP ="FILEFORMAT" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'" REQUIRED_IF="false">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TEXT_QUALIFIER" FIELD="TEXT" NUM_ROW="30" GROUP ="FILEFORMAT" SHOW_IF="(ISDELIMITED=='true') AND (#LINK@CONNECTOR.IN.ANY == '0')" REQUIRED_IF="false">
			<DEFAULT>"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TEXT_DELIMITER" FIELD="TEXT" NUM_ROW="35" GROUP ="FILEFORMAT" SHOW_IF="(ISDELIMITED=='true') AND (#LINK@CONNECTOR.IN.ANY == '0')" REQUIRED_IF="false">
			<DEFAULT>,</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SPLIT_ROWS" FIELD="CHECK" NUM_ROW="40" GROUP ="FILEFORMAT" SHOW_IF="(#LINK@CONNECTOR.IN.ANY == '0')" REQUIRED_IF="false">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>		
		<PARAMETER NAME="IGNORE_FIRST_RECORD" FIELD="CHECK" NUM_ROW="40" GROUP ="FILEFORMAT" SHOW_IF="(ISDELIMITED=='true') AND (#LINK@CONNECTOR.IN.ANY == '0')" REQUIRED_IF="false">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>	
		<PARAMETER NAME="ALLOW_SHORT_LINES" FIELD="CHECK" NUM_ROW="45" GROUP ="FILEFORMAT" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'" REQUIRED_IF="false">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="IGNORE_EXTRA_COLUMNS" FIELD="CHECK" NUM_ROW="50" GROUP ="FILEFORMAT" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'" REQUIRED_IF="false">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="ADVARGUMENTS" FIELD="TABLE" NUM_ROW="10"
			NB_LINES="5" SHOW="false">
			<ITEMS>
				<ITEM NAME="NAME" FIELD="TEXT">
				</ITEM>
				<ITEM NAME="VALUE" />
			</ITEMS>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
            <IMPORT MODULE_GROUP="camel-core" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-spring" REQUIRED="true" />
            <IMPORT MODULE_GROUP="camel-flatpack" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS />
</COMPONENT>
