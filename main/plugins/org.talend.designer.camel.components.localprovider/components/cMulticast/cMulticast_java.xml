<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Routing</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
	  <CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" COLOR="086438"/>   
		<CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1" MAX_OUTPUT="1"  NAME="BLOCK"/> 
  </CONNECTORS>

  <PARAMETERS>

	<PARAMETER
		NAME="URIS"
		FIELD="TABLE"
		NUM_ROW="1"
		NB_LINES="5"
		REQUIRED="true"
    >
      <ITEMS>
        <ITEM NAME="URI" VALUE="&quot;&quot;" FIELD="TEXT" />
	  </ITEMS>
	</PARAMETER>
	  
	<PARAMETER 
		NAME="USE_AGGREGATION_STRATEGY" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="5">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	  
	<PARAMETER NAME="AGGREGATION_STRATEGY" FIELD="TEXT" REQUIRED="true" NUM_ROW="5" SHOW_IF="USE_AGGREGATION_STRATEGY == 'true'">
		<DEFAULT>AggregationStrategy</DEFAULT>
	</PARAMETER>
	  
	<PARAMETER 
		NAME="PARALLEL_PROCESSING" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="2">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	  
	<PARAMETER
		NAME="USE_TIMEOUT"
		FIELD="CHECK"
		NUM_ROW="3"
		REQUIRED="true"
		SHOW_IF="(PARALLEL_PROCESSING == 'true')">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	 	  
	<PARAMETER
		NAME="TIMEOUT"
		FIELD="TEXT"
		NUM_ROW="4"
		REQUIRED="true"
		SHOW_IF="((PARALLEL_PROCESSING == 'true') and (USE_TIMEOUT == 'true'))">
	<DEFAULT>1000</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="STOP_ON_EXCEPTION" FIELD="CHECK" NUM_ROW="6">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>
   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   </ADVANCED_PARAMETERS>

	<CODEGENERATION>
	</CODEGENERATION>

   <RETURNS />
</COMPONENT>
