<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
	COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="false"
	SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Custom</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    	<CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1"/> 
		<CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1" MAX_OUTPUT="0" />  
  </CONNECTORS>

  <PARAMETERS>
		<PARAMETER NAME="FROM_REGISTRY" FIELD="RADIO" REQUIRED="true"
			NUM_ROW="2">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
    	<PARAMETER NAME="FROM_CLASS" FIELD="RADIO" REQUIRED="true"
			NUM_ROW="2">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
	<PARAMETER
      NAME="BEAN"
      FIELD="TEXT"
      NUM_ROW="10"
	  REQUIRED="true"
	  SHOW_IF="FROM_CLASS=='true'"
    >
		<DEFAULT>beans.MyBean.class</DEFAULT>
	</PARAMETER>
	<PARAMETER
      NAME="REF_ID"
      FIELD="TEXT"
      NUM_ROW="10"
	  REQUIRED="true"
	  SHOW_IF="FROM_REGISTRY=='true'"
    >
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	<PARAMETER 
		NAME="SPECIFY_METHOD" 
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER
      NAME="METHOD"
      FIELD="TEXT"
      NUM_ROW="20"
	  REQUIRED="true"
	  SHOW_IF="SPECIFY_METHOD == 'true'"
    >
		<DEFAULT>"Method"</DEFAULT>
	</PARAMETER>
  </PARAMETERS>

  <ADVANCED_PARAMETERS>
  </ADVANCED_PARAMETERS>

	<CODEGENERATION>
	</CODEGENERATION>

   <RETURNS />
</COMPONENT>
