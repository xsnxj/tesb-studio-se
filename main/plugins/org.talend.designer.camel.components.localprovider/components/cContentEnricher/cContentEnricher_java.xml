<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Transformation</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    	<CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1" MAX_OUTPUT="1" MIN_OUTPUT="1"/>
    	<CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1" MAX_OUTPUT="0" /> 
  </CONNECTORS>

   <PARAMETERS>
		<PARAMETER NAME="RESOURCE_URI" FIELD="TEXT" NUM_ROW="10"
			REQUIRED="true">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ENRICH" FIELD="RADIO" NUM_ROW="15"  GROUP="OPERATION">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="POLLENRICH" FIELD="RADIO" NUM_ROW="16"  GROUP="OPERATION">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_AGG_STRATEGY" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="AGGREGATION_STRATEGY" FIELD="TEXT" NUM_ROW="20"
			REQUIRED="true" SHOW_IF="USE_AGG_STRATEGY == 'true'">
			<DEFAULT>AggregationStrategy</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SPECIFY_TIMEOUT" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="25" SHOW_IF="POLLENRICH == 'true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="WAIT" FIELD="RADIO" NUM_ROW="26"  GROUP="TIMEOUT" SHOW_IF="(POLLENRICH == 'true') AND (SPECIFY_TIMEOUT == 'true')">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="IMMEDIATE" FIELD="RADIO" NUM_ROW="27"  GROUP="TIMEOUT" SHOW_IF="(POLLENRICH == 'true') AND (SPECIFY_TIMEOUT == 'true')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="TRIGGER" FIELD="RADIO" NUM_ROW="28"  GROUP="TIMEOUT" SHOW_IF="(POLLENRICH == 'true') AND (SPECIFY_TIMEOUT == 'true')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="TIMEOUT_TRIGGER" FIELD="TEXT" NUM_ROW="28"
			REQUIRED="true" SHOW_IF="(POLLENRICH == 'true') AND (SPECIFY_TIMEOUT == 'true') AND (TRIGGER == 'true')" GROUP="TIMEOUT">
			<DEFAULT>1000</DEFAULT>
		</PARAMETER>
   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   </ADVANCED_PARAMETERS>

   <CODEGENERATION/>

   <RETURNS />
</COMPONENT>
