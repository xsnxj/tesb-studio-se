<COMPONENT>

    <HEADER 
        PLATEFORM="ALL"
        SERIAL=""
        VERSION="0.1"
        STATUS="ALPHA"
        COMPATIBILITY="ALL"
        AUTHOR="Talend"
        RELEASE_DATE="20160914A"
        STARTABLE="true">
        
        <SIGNATURE />
    </HEADER>

    <FAMILIES>
        <FAMILY>AWS</FAMILY>
        <FAMILY>Connectivity/Messaging</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="ROUTE" MIN_INPUT="0" MAX_INPUT="1"
			MAX_OUTPUT="1" MIN_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1" MAX_OUTPUT="0" />
	</CONNECTORS>

    <PARAMETERS>
        <PARAMETER NAME="AWS_CONNECTION_CLIENT" FIELD="ROUTE_COMPONENT_TYPE" FILTER="cAWSConnection&amp;AWS_TYPE == 'SQS'" NUM_ROW="1"/>

        <PARAMETER NAME="QUEUE_NAME" FIELD="TEXT" NUM_ROW="10" REQUIRED="true">
            <DEFAULT>""</DEFAULT>
            <!-- queue name or queue arn 2.18+ (arn:aws:sqs:region:account:name) -->
        </PARAMETER>

        <PARAMETER NAME="DELETE_AFTER_READ" FIELD="CHECK" NUM_ROW="20" GROUP="DELETE_OPTIONS" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>true</DEFAULT>
            <!-- deleteAfterRead enabled -->
        </PARAMETER>
        <PARAMETER NAME="DELETE_IF_FILTERED" FIELD="CHECK" NUM_ROW="20" GROUP="DELETE_OPTIONS" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>true</DEFAULT>
            <!-- deleteIfFiltered enabled -->
        </PARAMETER>
        
        
        <PARAMETER NAME="ENABLE_CONCURRENT" FIELD="CHECK" NUM_ROW="30" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>false</DEFAULT>
            <!-- concurrentConsumers enabled -->
        </PARAMETER>

        <PARAMETER NAME="POOL_NUMBER" FIELD="TEXT" NUM_ROW="30" REQUIRED="true" SHOW_IF="(ENABLE_CONCURRENT == 'true') AND (#LINK@CONNECTOR.IN.ANY == '0')">
            <DEFAULT>1</DEFAULT>
            <!-- concurrentConsumers -->
        </PARAMETER>

        <PARAMETER NAME="MAX_MESSAGE" FIELD="TEXT" NUM_ROW="30" REQUIRED="true" SHOW_IF="(ENABLE_CONCURRENT == 'true') AND (#LINK@CONNECTOR.IN.ANY == '0')">
          <DEFAULT>1</DEFAULT>
          <!-- maxMessagesPerPoll -->
        </PARAMETER>
        
        
        <PARAMETER NAME="EXTEND_MESSAGE_VISIBILITY" FIELD="CHECK" NUM_ROW="40" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
            <DEFAULT>false</DEFAULT>
            <!-- extendMessageVisibility enabled -->
        </PARAMETER>
        
        <PARAMETER NAME="VISIBILITY_TIMEOUT" FIELD="TEXT" NUM_ROW="40" REQUIRED="false" SHOW_IF="(EXTEND_MESSAGE_VISIBILITY == 'true') AND (#LINK@CONNECTOR.IN.ANY == '0')">
          <DEFAULT>30</DEFAULT>
          <!-- visibilityTimeout -->
        </PARAMETER>

        <PARAMETER NAME="DELAY_SECOND" FIELD="TEXT" NUM_ROW="50" REQUIRED="false" SHOW_IF="#LINK@CONNECTOR.IN.ANY &gt; '0'">
          <DEFAULT>0</DEFAULT>
          <!-- delaySeconds -->
        </PARAMETER>
        <PARAMETER NAME="WAIT_TIME_SECOND" FIELD="TEXT" NUM_ROW="60" REQUIRED="false" SHOW_IF="#LINK@CONNECTOR.IN.ANY &gt; '0'">
          <DEFAULT>0</DEFAULT>
          <!-- waitTimeSeconds -->
        </PARAMETER>
        
        <PARAMETER NAME="ATTRIBUTE_ALL" FIELD="CHECK" NUM_ROW="70" REQUIRED="false" GROUP="ATTRIBUTE_GROUP" SHOW_IF="(#LINK@CONNECTOR.IN.ANY == '0')">
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="ATTRIBUTE_RECEIVE_TIME" FIELD="CHECK" NUM_ROW="72" REQUIRED="false" GROUP="ATTRIBUTE_GROUP" SHOW_IF="(#LINK@CONNECTOR.IN.ANY == '0') AND (ATTRIBUTE_ALL == 'false')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="ATTRIBUTE_RECEIVE_COUNT" FIELD="CHECK" NUM_ROW="73" REQUIRED="false" GROUP="ATTRIBUTE_GROUP" SHOW_IF="(#LINK@CONNECTOR.IN.ANY == '0') AND (ATTRIBUTE_ALL == 'false')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="ATTRIBUTE_SENDER_ID" FIELD="CHECK" NUM_ROW="74" REQUIRED="false" GROUP="ATTRIBUTE_GROUP" SHOW_IF="(#LINK@CONNECTOR.IN.ANY == '0') AND (ATTRIBUTE_ALL == 'false')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="ATTRIBUTE_SENT_TIME" FIELD="CHECK" NUM_ROW="75" REQUIRED="false" GROUP="ATTRIBUTE_GROUP" SHOW_IF="(#LINK@CONNECTOR.IN.ANY == '0') AND (ATTRIBUTE_ALL == 'false')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
   </PARAMETERS>

    <ADVANCED_PARAMETERS>
        <PARAMETER NAME="QUEUE_OWNER_ACCOUNT" FIELD="TEXT" NUM_ROW="2" REQUIRED="false">
          <DEFAULT>""</DEFAULT>
        </PARAMETER>
        
        <PARAMETER NAME="QUEUE_SETTING" FIELD="CHECK" NUM_ROW="10" REQUIRED="false" GROUP="QUEUE_GROUP">
            <DEFAULT>false</DEFAULT>
            <!-- org.apache.camel.component.aws.sqs.SqsEndpoint protected void createQueue(AmazonSQS client) -->
        </PARAMETER>
        <PARAMETER NAME="QUEUE_CONFIGURATION" FIELD="TABLE" NUM_ROW="11" NB_LINES="5" REQUIRED="true" SHOW_IF="QUEUE_SETTING=='true'" GROUP="QUEUE_GROUP">
          <ITEMS>
            <ITEM NAME="QUEUE_PARAMETER" FIELD="CLOSED_LIST">
                <ITEMS DEFAULT="DEFAULTVISIBILITYTIMEOUT">
                    <ITEM NAME="DEFAULTVISIBILITYTIMEOUT" VALUE="defaultVisibilityTimeout"/>
                    <ITEM NAME="MAXIMUMMESSAGESIZE" VALUE="maximumMessageSize"/>
                    <ITEM NAME="MESSAGERETENTIONPERIOD" VALUE="messageRetentionPeriod"/>
                    <ITEM NAME="POLICY" VALUE="policy"/>
                    <ITEM NAME="RECEIVEMESSAGEWAITTIMESECONDS" VALUE="receiveMessageWaitTimeSeconds"/>
                    <ITEM NAME="REDRIVEPOLICY" VALUE="redrivePolicy"/>
                </ITEMS>
            </ITEM>
            <ITEM NAME="VALUE" />
          </ITEMS>
        </PARAMETER>

        <PARAMETER NAME="MESSAGE_ATTRIBUTE_NAME" FIELD="TABLE" NUM_ROW="40" REQUIRED="false" GROUP="MESSAGE_ATTRIBUTE_GROUP" SHOW_IF="#LINK@CONNECTOR.IN.ANY == '0'">
          <ITEMS>
            <ITEM NAME="ATTRIBUTE_NAME"  VALUE="&quot;&quot;" />
          </ITEMS>
        </PARAMETER>
        
    </ADVANCED_PARAMETERS>
	<CODEGENERATION />
    <RETURNS />
</COMPONENT>
